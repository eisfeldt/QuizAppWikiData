#Animals 
SELECT distinct ?animal ?animalLabel ?taxonname ?food ?foodLabel # ?species ?speciesLabel 
{
  ?animal wdt:P31 wd:Q16521 .
  ?animal wdt:P225 ?taxonname;
          wdt:P1034 ?food;
          # wdt:P279 ?species; Gibt "animal" aus, aber zu selten gepflegt

  SERVICE wikibase:label {
    bd:serviceParam wikibase:language "en" .

  }
} ORDER BY ?animal 
Limit 50
